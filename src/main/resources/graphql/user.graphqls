type User {
  id: ID!
  name: String!
  password: String!
  email: String!
  createdAt: String!
  updatedAt: String!
}

type Query {
  findAllUsers: [User]!
  countUsers: Long!
}

type Subscription {
  userSubscribe: Subscribe!
}

type Mutation {
  newUser(name: String!, password: String!, email: String!): User!
}
